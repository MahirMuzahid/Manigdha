<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Manigdha.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Manigdha.View"
    FlyoutBackdrop="{AppThemeBinding Light={StaticResource Gray300}, Dark={StaticResource Gray600}}"
    Shell.FlyoutBehavior="Disabled">



    <Tab Title="Home">
        <ShellContent
        ContentTemplate="{DataTemplate local:BuyPost}"
        Route="BuyPost" />
    </Tab>
   
    <Tab Title="Profile">
        <ShellContent
        ContentTemplate="{DataTemplate local:Profile}"
        Route="Profile" />
    </Tab>
    <Tab Title="Notification">
        <ShellContent
        ContentTemplate="{DataTemplate local:Notification}"
        Route="Notification" />
    </Tab>

   



</Shell>
